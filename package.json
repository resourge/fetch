{
  "name": "@resourge/fetch",
  "description": "This is a set of small packages aiming to simplify the pagination process.",
  "private": false,
  "version": "0.0.0-development",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "fetch",
    "api",
    "throttle",
    "react",
    "hooks",
    "react-fetch"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/fetch.git"
  },
  "scripts": {
    "commit": "git pull && git add . && git-cz",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "dev": "vite",
    "build:vite": "vite build",
    "injectBannerIntoDeclarations": "ts-node-esm ./scripts/injectBannerIntoDeclarations.ts",
    "fixReactFetch": "ts-node-esm ./scripts/fixReactFetch.ts",
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-native": "^0.72.2",
    "@types/use-sync-external-store": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-dev-expression": "^0.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-newlines": "^1.3.4",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "fast-glob": "^3.3.1",
    "jsdom": "^22.1.0",
    "minimist": "^1.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recursive-copy": "^2.0.14",
    "rollup": "^3.29.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-shell": "^1.0.9",
    "semantic-release": "^21.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
