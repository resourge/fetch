{
  "name": "@resourge/fetch",
  "description": "This is a set of small packages aiming to simplify the pagination process.",
  "private": false,
  "version": "0.0.0-development",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "javascript",
    "fetch",
    "api",
    "throttle",
    "react",
    "hooks",
    "react-fetch"
  ],
  "files": [
    "dist"
  ],
  "author": "resourge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resourge/fetch.git"
  },
  "scripts": {
    "commit": "git pull && git add . && git-cz",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:prod": "cross-env NODE_ENV=production npm run lint",
    "dev": "vite",
    "build:vite": "vite build",
    "injectBannerIntoDeclarations": "ts-node-esm ./scripts/injectBannerIntoDeclarations.ts",
    "fixReactFetch": "ts-node-esm ./scripts/fixReactFetch.ts",
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-native": "^0.71.3",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-newlines": "^1.3.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "fast-glob": "^3.2.12",
    "jsdom": "^21.1.0",
    "minimist": "^1.2.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recursive-copy": "^2.0.14",
    "rollup": "^3.18.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-shell": "^1.0.9",
    "semantic-release": "^20.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
